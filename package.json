{
  "name": "pge-wall",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-file-reader": "^1.1.4",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "node server.js",
    "stop": "(kill -9 $(lsof -ti :5000) 2> /dev/null); (kill -9 $(lsof -ti :3000) 2> /dev/null); echo 'Stopped'",
    "local": "npm run build && (node server.js & open http://localhost:5000)",
    "restart": "(kill -9 $(lsof -ti :5000) 2> /dev/null); node server.js &",
    "dev": "node server.js & react-scripts start &",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
